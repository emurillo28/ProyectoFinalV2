<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ivan.proyectofinalv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnterActivityData"
            android:label="@string/enterdata"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SelectActivity"
            android:label="Elige la actividad"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".HistoryActivity"
            android:label="Registro de actividades"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SummaryActvity"
            android:label="Registro de actividades"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AgregarCategoria"
            android:label="@string/title_activity_agregar_categoria"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".GraficaListActivity"
            android:label="@string/title_grafica_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".GraficaDetailActivity"
            android:label="@string/title_grafica_detail"
            android:parentActivityName=".GraficaListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ivan.proyectofinalv2.GraficaListActivity" />
        </activity>
    </application>

</manifest>